{{ '## vconsole.conf' }}
{{ '# ' + ansible_managed }}
{{ '# Do not edit manually' }}

{% for c in bootstrap_linux_systemd__vconsole %}
{% if c.keymap is defined and c.keymap != '' %}
{{ 'KEYMAP=\"' + c.keymap + '\"' }}
{% endif %}
{% if c.keymap_toggle is defined and c.keymap_toggle != '' %}
{{ 'KEYMAP_TOGGLE=\"' + c.keymap_toggle + '\"' }}
{% endif %}
{% if c.font is defined and c.font != '' %}
{{ 'FONT=\"' + c.font + '\"' }}
{% endif %}
{% if c.font_map is defined and c.font_map != '' %}
{{ 'FONT_MAP=\"' + c.font_map + '\"' }}
{% endif %}
{% if c.font_unimap is defined and c.font_unimap != '' %}
{{ 'FONT_UNIMAP=\"' + c.font_unimap + '\"' }}
{% endif %}
{% endfor %}
