client
dev tun
proto {{ __docker_stack_appspecs__media_openvpn_proto }}
remote {{ __docker_stack_appspecs__media_openvpn_ip }} {{ __docker_stack_appspecs__media_openvpn_port }}
resolv-retry infinite
remote-random
nobind
tun-mtu 1500
tun-mtu-extra 32
mssfix 1450
persist-key
persist-tun
ping 15
ping-restart 0
ping-timer-rem
reneg-sec 0
comp-lzo no

explicit-exit-notify 3

remote-cert-tls server

#mute 10000
#auth-user-pass
auth-user-pass /vpn/pass.txt

